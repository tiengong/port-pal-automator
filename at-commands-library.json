  "urcPatterns": [
    {
      "pattern": "+CREG:",
      "description": "网络注册状态变化",
      "category": "网络"
    },
    {
      "pattern": "+CGREG:",
      "description": "GPRS网络注册状态变化",
      "category": "网络"
    },
    {
      "pattern": "+CEREG:",
      "description": "EPS网络注册状态变化",
      "category": "网络"
    },
    {
      "pattern": "+CMTI:",
      "description": "新短信到达指示",
      "category": "短信"
    },
    {
      "pattern": "+CMT:",
      "description": "新短信直接显示",
      "category": "短信"
    },
    {
      "pattern": "+CDSI:",
      "description": "新状态报告到达",
      "category": "短信"
    },
    {
      "pattern": "+CDS:",
      "description": "状态报告直接显示",
      "category": "短信"
    },
    {
      "pattern": "+CGEV:",
      "description": "PDP上下文事件",
      "category": "数据"
    },
    {
      "pattern": "RING",
      "description": "来电指示",
      "category": "通话"
    },
    {
      "pattern": "+CRING",
      "description": "来电指示（扩展）",
      "category": "通话"
    },
    {
      "pattern": "+CLIP:",
      "description": "来电显示",
      "category": "通话"
    },
    {
      "pattern": "NO CARRIER",
      "description": "无载波/连接断开",
